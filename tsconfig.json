{
    "compilerOptions": {
        "lib": [
            "ES2019",
            "ES2019.array"
        ],
        "module": "commonjs",
        "target": "ES2019",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist"
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "./node_modules"
    ],
}